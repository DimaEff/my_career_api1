{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/flashcall/src",
  "monorepo": true,
  "root": "apps/flashcall",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/flashcall/tsconfig.app.json"
  },
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "flashcall": {
      "type": "application",
      "root": "apps/flashcall",
      "entryFile": "main",
      "sourceRoot": "apps/flashcall/src",
      "compilerOptions": {
        "tsConfigPath": "apps/flashcall/tsconfig.app.json"
      }
    },
    "mongodb": {
      "type": "library",
      "root": "libs/mongodb",
      "entryFile": "index",
      "sourceRoot": "libs/mongodb/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mongodb/tsconfig.lib.json"
      }
    },
    "file-storage": {
      "type": "library",
      "root": "libs/file-storage",
      "entryFile": "index",
      "sourceRoot": "libs/file-storage/src",
      "compilerOptions": {
        "tsConfigPath": "libs/file-storage/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}